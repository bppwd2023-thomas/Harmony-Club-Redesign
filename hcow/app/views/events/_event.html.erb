<div id="<%= dom_id event %>">
<table class="table">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Details</th>
      <th scope="col">Occurence</th>
      <th scope="col">Start</th>
      <th scope="col">End</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row"><%= event.title %></th>
      <td><%=event.details %></td>
      <td><%=event.occurence %></td>
      <td><%=event.eventstart %></td>
      <td><%=event.eventend %></td>
    </tr>
  </tbody>
</table>
</div>
